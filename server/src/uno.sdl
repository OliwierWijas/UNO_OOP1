type ReturnGameDTO {
  name: String!
}

input CreateGameDTO {
  name: String!
}

type Query {
  get_games: [ReturnGameDTO!]!
  get_pending_games: [ReturnGameDTO!]!
}

type Mutation {
  create_game(game: CreateGameDTO) : ReturnGameDTO
}

type Subscription {
  pending_games_updated: [ReturnGameDTO!]!,
}